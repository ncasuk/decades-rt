$!$! COMMAND. - Executes commands passed in the query string$!$ SET DEFAULT SYS$SYSDEVICE:[HORACE]$ WRITE SYS$OUTPUT "content-type: text/plain"$ WRITE SYS$OUTPUT ""$!$ IF QUERY_STRING .EQS. "h_stat"$ THEN$   @HORACE:H_STAT$   EXIT$ ENDIF$!$ IF QUERY_STRING .EQS. "show_sys"$ THEN$   SHOW SYS$   EXIT$ ENDIF$!$ IF QUERY_STRING .EQS. "dir"$ THEN$   DIR /DAT /SIZ=ALL /WIDTH=FILENAME=25$   EXIT$ ENDIF$!$ IF QUERY_STRING .EQS. "check_sys"$ THEN$   RUN H_CHECK_SYS$   EXIT$ ENDIF$!$ IF QUERY_STRING .EQS. "check_para"$ THEN$   RUN H_INSTQC$   EXIT$ ENDIF